extend type Query {
    getAllWeapons: [Weapon]
    getWeaponById(id: ID): Weapon
}

type Weapon {
    id: ID!
    page: String!
    name: String!
    image: String!
    type: String
    overallLength: Float
    bladeLength: Float
    bladeWidth: Float
    cob: Float
    cop: Float
    weight: Float
    axEdgeLength: Float
    axWarHeadDepth: Float
}