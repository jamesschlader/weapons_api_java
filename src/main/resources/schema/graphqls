type Query {
    getAllWeapons: [Weapon]
    getWeaponById(id: ID): Weapon
}

type Weapon {
    id: ID!
    page: String!
    name: String!
    image: String!
    type: String
    overall_length: Float
    blade_length: Float
    blade_width: Float
    cob: Float
    cop: Float
    weight: Float
    ax_edge_length: Float
    ax_war_head_depth: Float
}